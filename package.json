  "description": "Generate physics based css-keyframe animations",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codepunkt/css-spring.git"
  },
  "author": "Christoph Werner <christoph@codepunkt.de>",
  "license": "MIT",
  "keywords": [
    "react",
