    "coveralls": "^2.11.15",
    "cross-env": "^3.1.4",
    "eslint": "^3.14.1",
    "eslint-config-xo": "^0.17.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-unicorn": "^1.0.0",
